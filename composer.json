{
    "description": "Fixtures bundle",
    "name": "atournayre/fixtures-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "hautelook/alice-bundle": "^2.12",
        "symfony/config": "^6.2 || ^7.0",
        "symfony/dependency-injection": "^6.2 || ^7.0",
        "symfony/http-kernel": "^6.2 || ^7.0",
        "symfony/password-hasher": "^6.2 || ^7.0",
        "symfony/uid": "^6.2 || ^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\FixtureBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\FixtureBundle\\Tests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
